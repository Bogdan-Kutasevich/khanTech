{
    "env": {
        "browser": true,
        "es2021": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "extends": "airbnb-base",
    "rules": {
        "import/prefer-default-export": "off",
        "class-methods-use-this": "off",
        "import/extensions": "off"
    }
}
